{
    "allowCustomDataForDataTypeAny": true,
    "functions": [
        {
            "description": "Pulls time series data from Seeq sensors over a specified time range.\nThis is an array function that should be called on a range that can accommodate the output.",
            "id": "PULL",
            "name": "PULL",
            "parameters": [
                {
                    "description": "Range containing sensor names (e.g., B1:D1)",
                    "dimensionality": "matrix",
                    "name": "sensorNames",
                    "type": "string"
                },
                {
                    "description": "Start time for data pull (ISO format: \"2024-01-01T00:00:00\" or \"8/1/2025 0:00\")",
                    "name": "startDatetime",
                    "type": "string"
                },
                {
                    "description": "End time for data pull (ISO format: \"2024-01-31T23:59:59\" or \"8/3/2025 0:00\")",
                    "name": "endDatetime",
                    "type": "string"
                },
                {
                    "description": "Data retrieval mode: \"grid\" for time-based intervals or \"points\" for number of points - defaults to \"points\"",
                    "name": "mode",
                    "optional": true,
                    "type": "string"
                },
                {
                    "description": "Grid interval (e.g., \"15min\", \"1h\", \"1d\") when mode=\"grid\" OR number of points when mode=\"points\" - defaults to 1000",
                    "name": "modeValue",
                    "optional": true,
                    "type": "any"
                }
            ],
            "result": {
                "dimensionality": "matrix",
                "type": "string"
            }
        },
        {
            "description": "Searches for sensors in Seeq without pulling data.",
            "id": "SEARCH_SENSORS",
            "name": "SEARCH_SENSORS",
            "parameters": [
                {
                    "description": "Range containing sensor names (e.g., B1:D1)",
                    "dimensionality": "matrix",
                    "name": "sensorNames",
                    "type": "string"
                }
            ],
            "result": {
                "dimensionality": "matrix",
                "type": "string"
            }
        },
        {
            "description": "Gets the current value of a sensor at the present time.\nReturns a single-cell scalar value.",
            "id": "CURRENT",
            "name": "CURRENT",
            "parameters": [
                {
                    "description": "Name of the sensor (e.g., \"Area/Tag\")",
                    "name": "sensorName",
                    "type": "string"
                }
            ],
            "result": {}
        },
        {
            "description": "Computes the average value of a sensor over a time range using a 100-point grid.\nReturns a single-cell scalar value.",
            "id": "AVERAGE",
            "name": "AVERAGE",
            "parameters": [
                {
                    "description": "Name of the sensor (e.g., \"Area/Tag\")",
                    "name": "sensorName",
                    "type": "string"
                },
                {
                    "description": "Start time (e.g., \"2024-01-01T00:00:00\" or \"8/1/2025 0:00\")",
                    "name": "startDatetime",
                    "type": "string"
                },
                {
                    "description": "End time (e.g., \"2024-01-31T23:59:59\" or \"8/1/2025 1:40\")",
                    "name": "endDatetime",
                    "type": "string"
                }
            ],
            "result": {}
        },
        {
            "description": "Creates Python plotting code with embedded sensor data for visualization.\nThis function fetches sensor data and returns complete Python code as text.",
            "id": "CREATE_PLOT_CODE",
            "name": "CREATE_PLOT_CODE",
            "parameters": [
                {
                    "description": "Range containing sensor names (e.g., B1:D1)",
                    "dimensionality": "matrix",
                    "name": "sensorNames",
                    "type": "string"
                },
                {
                    "description": "Start time for data pull (ISO format: \"2024-01-01T00:00:00\" or \"8/1/2025 0:00\")",
                    "name": "startDatetime",
                    "type": "string"
                },
                {
                    "description": "End time for data pull (ISO format: \"2024-01-31T23:59:59\" or \"8/3/2025 0:00\")",
                    "name": "endDatetime",
                    "type": "string"
                },
                {
                    "description": "Number of data points to retrieve (defaults to 100)",
                    "name": "points",
                    "optional": true,
                    "type": "number"
                },
                {
                    "description": "Plot height in inches (defaults to 0.3)",
                    "name": "height",
                    "optional": true,
                    "type": "number"
                },
                {
                    "description": "Width to height ratio (defaults to 5)",
                    "name": "aspectRatio",
                    "optional": true,
                    "type": "number"
                },
                {
                    "description": "Whether to show connecting lines (defaults to true)",
                    "name": "showLine",
                    "optional": true,
                    "type": "boolean"
                },
                {
                    "description": "Whether to show data points (defaults to true)",
                    "name": "showPoints",
                    "optional": true,
                    "type": "boolean"
                },
                {
                    "description": "Point and line opacity 0-1 (defaults to 0.9)",
                    "name": "opacity",
                    "optional": true,
                    "type": "number"
                },
                {
                    "description": "Plot color - use color names like 'green', 'blue', 'red' or hex codes (defaults to 'green')",
                    "name": "color",
                    "optional": true,
                    "type": "string"
                },
                {
                    "description": "Plot style: 'normal', 'minimal', or 'sparkline' (defaults to 'sparkline')",
                    "name": "style",
                    "optional": true,
                    "type": "string"
                },
                {
                    "description": "Y-axis label (defaults to 'Value')",
                    "name": "label",
                    "optional": true,
                    "type": "string"
                }
            ],
            "result": {
                "type": "string"
            }
        }
    ]
}